				<Source><![CDATA[
    public void updateDesign()
    {
        boolean existsBatchAttrib       = inventTestGroupMember.PdsBatchAttribId || inventQualityOrderLine.PdsBatchAttribId;
        boolean excludeFromCOA          = pipInventTestCertOfAnalysisCustReq.ExcludeFromCOA;
        boolean suppressMinMaxValues    = pipInventTestCertOfAnalysisCustReq.SupressMinMaxValues;
        boolean useCustBatchAttribRange = pipInventTestCertOfAnalysisCustReq.UseCustBatchAttribRange;
    
        pipInventTestCertOfAnalysisCustReq_ds.object(fieldNum(PIPInventTestCertOfAnalysisCustReq, CustomerRelation)).allowEdit(pipInventTestCertOfAnalysisCustReq.CustomerCode != TableGroupAll::All);
        pipInventTestCertOfAnalysisCustReq_ds.object(fieldNum(PIPInventTestCertOfAnalysisCustReq, ReplacePassResults)).allowEdit(!excludeFromCOA);
        pipInventTestCertOfAnalysisCustReq_ds.object(fieldNum(PIPInventTestCertOfAnalysisCustReq, ReplaceFailResults)).allowEdit(!excludeFromCOA);
        pipInventTestCertOfAnalysisCustReq_ds.object(fieldNum(PIPInventTestCertOfAnalysisCustReq, SupressMinMaxValues)).allowEdit(!excludeFromCOA && !useCustBatchAttribRange);
        pipInventTestCertOfAnalysisCustReq_ds.object(fieldNum(PIPInventTestCertOfAnalysisCustReq, UseCustBatchAttribRange)).allowEdit(!excludeFromCOA && existsBatchAttrib && !suppressMinMaxValues);
    
        pipInventTestCertOfAnalysisCustReq_ds.object(fieldNum(PIPInventTestCertOfAnalysisCustReq, CustomerRelation)).skip(pipInventTestCertOfAnalysisCustReq.CustomerCode == TableGroupAll::All);
        pipInventTestCertOfAnalysisCustReq_ds.object(fieldNum(PIPInventTestCertOfAnalysisCustReq, ReplacePassResults)).skip(excludeFromCOA);
        pipInventTestCertOfAnalysisCustReq_ds.object(fieldNum(PIPInventTestCertOfAnalysisCustReq, ReplaceFailResults)).skip(excludeFromCOA);
        pipInventTestCertOfAnalysisCustReq_ds.object(fieldNum(PIPInventTestCertOfAnalysisCustReq, SupressMinMaxValues)).skip(excludeFromCOA || useCustBatchAttribRange);
        pipInventTestCertOfAnalysisCustReq_ds.object(fieldNum(PIPInventTestCertOfAnalysisCustReq, UseCustBatchAttribRange)).skip((excludeFromCOA || suppressMinMaxValues) && !existsBatchAttrib);
    
        BatchAttributesTestGroupMember.enabled(existsBatchAttrib);
        BatchAttributesQOLine.enabled(existsBatchAttrib);
        PdsBatchAttribByitemCustomerCOA.enabled(pipInventTestCertOfAnalysisCustReq.CustomerCode == TableGroupAll::Table);
    }

]]></Source>
