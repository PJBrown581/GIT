			<Method>
				<Name>mapEntityToDataSource</Name>
				<Source><![CDATA[
    /// <summary>
    /// Refreshes the billing classification data source for the free text invoice header entity.
    /// Also ensures basic defaulting takes place on insert.
    /// </summary>
    /// <param name = "_entityCtx">
    /// The runtime context for free text invoice header entity.
    /// </param>
    /// <param name = "_dataSourceCtx">
    /// The runtime context of the data source for the free text invoice header entity.
    /// </param>
    public void mapEntityToDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        if (_entityCtx.getDatabaseOperation() == DataEntityDatabaseOperation::Insert
            || _entityCtx.getDatabaseOperation() == DataEntityDatabaseOperation::Update)
        {
            switch (_dataSourceCtx.name())
            {
                case dataEntityDataSourceStr(FreeTextInvoiceHeaderEntity, CustBillingClassification):
                    if (CustParameters::isBillingClassificationEnabled())
                    {
                        CustBillingClassification currentBillingClassification = _dataSourceCtx.getBuffer();

                        currentBillingClassification.BillingClassification = this.BillingClassification;
                        currentBillingClassification.RecId = this.BillingClassificationId;

                        _dataSourceCtx.setBuffer(currentBillingClassification);
                    }
                    break;

                case dataEntityDataSourceStr(FreeTextInvoiceHeaderEntity, CustInvoiceTable):                    
                    if (_entityCtx.getDatabaseOperation() == DataEntityDatabaseOperation::Insert)
                    {
                        CustInvoiceTable custInvoiceTable = _dataSourceCtx.getBuffer();
                        custInvoiceTable.initFromCustTable(CustTable::find(this.CustomerAccount));
                        custInvoiceTable.initFromTaxItemGroup();
                    }
                    break;
            }
        }

        super(_entityCtx, _dataSourceCtx);
    }

]]></Source>
			</Method>
