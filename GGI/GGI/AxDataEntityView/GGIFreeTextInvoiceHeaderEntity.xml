			<Method>
				<Name>generateFreeTextInvoiceNumber</Name>
				<Source><![CDATA[
    /// <summary>
    /// Generates Free text number which is unique for all invoices being exported or imported in scope of each DataAreaId.
    /// </summary>
    /// <returns>
    /// If export of posted invoices is done then it returns just the invoice number itself.
    /// Otherwise sets it to be "Not posted 1", "Not posted 2" and etc.
    /// </returns>
    public static str generateFreeTextInvoiceNumber()
    {
        str sql = 'IIF(';

        sql += SysComputedColumn::returnField(tablestr(FreeTextInvoiceHeaderEntity), dataEntityDataSourceStr(FreeTextInvoiceHeaderEntity, CustInvoiceTable), fieldstr(FreeTextInvoiceHeaderEntity, InvoiceId));

        sql += '=\'\',\'';
        sql += "@SYS11670";
        sql += '\',';
        sql += SysComputedColumn::returnField(tableStr(FreeTextInvoiceHeaderEntity), dataEntityDataSourceStr(FreeTextInvoiceHeaderEntity, CustInvoiceTable), fieldStr(FreeTextInvoiceHeaderEntity, InvoiceId));
        sql += ')';

        return sql;
    }

]]></Source>
			</Method>
			<Method>
				<Name>validateDelete</Name>
				<Source><![CDATA[
    public boolean validateDelete()
    {
        boolean ret = super();

        if (this.InvoiceIdentifier && !CustInvoiceTable::findRecId(this.InvoiceIdentifier).isEditable())
        {
            return checkFailed("@AccountsReceivable:FreeTextInvoiceHeaderEntityCheckDelete");
        }

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>validateWrite</Name>
				<Source><![CDATA[
    public boolean validateWrite()
    {
        boolean ret = super();

        if (this.InvoiceIdentifier && !CustInvoiceTable::findRecId(this.InvoiceIdentifier).isEditable())
        {
            return checkFailed("@AccountsReceivable:FreeTextInvoiceHeaderEntityCheckWrite");
        }

        if (CustParameters::isBillingClassificationEnabled() && ret)
        {
            //
            // Need to validate all billing code on invoice lines
            // Billing codes must be valid for the billing classification and invoice date
            //

            CustBillingCodeVersion billingCodeVersion;
            CustBillingClassificationCode billingClassificationCode;
            CustInvoiceLine invoiceLine;

            if (this.BillingClassification == "")
            {
                return checkFailed("@AccountsReceivable:FreeTextInvoiceHeaderEntityCheckBillingClassificatonEmpty");
            }

            RefRecId currentBillingClassificationRecId = CustBillingClassification::findByBillingClassification(this.BillingClassification).RecId;

            select firstonly RecId from invoiceLine
                where invoiceLine.ParentRecId == this.InvoiceIdentifier
                    && invoiceLine.CustBillingCode > 0
                notexists join billingClassificationCode
                    where billingClassificationCode.CustBillingCode == invoiceLine.CustBillingCode
                        && billingClassificationCode.CustBillingClassification == currentBillingClassificationRecId;

            if (invoiceLine.RecId > 0)
            {
                return checkFailed("@AccountsReceivable:FreeTextInvoiceHeaderEntityCheckBillingClassificaton");
            }

            date currentInvoiceDate = this.InvoiceDate;

            select firstonly validtimestate(currentInvoiceDate) RecId from invoiceline
                where invoiceLine.ParentRecId == this.InvoiceIdentifier
                    && invoiceLine.CustBillingCode > 0
                notexists join billingCodeVersion
                    where billingCodeVersion.CustBillingCode == invoiceLine.CustBillingCode;

            if (invoiceLine.RecId > 0)
            {
                return checkFailed("@AccountsReceivable:FreeTextInvoiceHeaderEntityCheckInvoiceDate");
            }
        }

        return ret;
    }

]]></Source>
