				<Source><![CDATA[
    public void mapEntityToDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        _dataSourceCtx.setBuffer(SystemParameters::find());
        _dataSourceCtx.setDatabaseOperation(DataEntityDatabaseOperation::Update);

        PIPInitEntityHelper::populateDefaultData();

        super(_entityCtx, _dataSourceCtx);

        SystemParameters parameters = _dataSourceCtx.getBuffer();
        if (!parameters.SystemLanguageId)
        {
            parameters.SystemLanguageId = 'en-us'; //Default system language if no mapping was given
        }
    }

]]></Source>
