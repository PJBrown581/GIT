				<Source><![CDATA[
[Form]
public class PIPApprovedItemsLookup extends FormRun
{
    #define.TabInit(0)
    #define.TabApproved(1)
    #define.TabAll(2)
    
    CustTable               custTable;
    Map                     tab2ComboBoxItemMap;
    ValidFromDate           fromDate;
    ValidToDate             toDate;
    boolean                 approvedItemsListInitialized;
    str                     itemFilterStr;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        switch (element.args().dataset())
        {
            case (tableNum(SalesLine)):
            case (tableNum(SalesQuotationLine)):
            case (tableNum(AgreementLine)):
            case (tableNum(AgreementLineQuantityCommitment)):
            case (tableNum(AgreementLineVolumeCommitment)):
                break;
    
            default:
                throw error(error::missingFormActiveBuffer(this.name(),
                    strFmt('%1, %2, %3', tableStr(SalesLine), tableStr(SalesQuotationLine), tableStr(AgreementLine))));
        }

        itemFilterStr = element.args().parm();
    
        super();
    
        if (smmParametersTable::find().PIPACLItemLookupMethod == PIPACLItemLookupMethod::AllItemsFirst)
        {
            ctrlTab.tab(#TabAll);
        }
        
        tab2ComboBoxItemMap = SysLookup::tab2ComboBox(ctrlTab, switchView);

        this.setSelectMode();
    }

]]></Source>
			</Method>
			<Method>
				<Name>setSelectMode</Name>
				<Source><![CDATA[
    void setSelectMode()
    {
        int currentTable = ctrlTab.tab();
    
        switch (ctrlTab.tab())
        {
            case #TabInit, #TabApproved:
                if (!approvedItemsListInitialized)
                {
                    PIPApprovedCustomerList::getApprovedItems(custTable, fromDate, toDate, TmpPIPApprovedItems, itemFilterStr);
                    TmpPIPApprovedItems_ds.research();
                    approvedItemsListInitialized = true;
                }

                this.selectMode(TmpPIPApprovedItems_ItemId);
                TmpPIPApprovedItems_ItemId.setFocus();
                break;
    
            case #TabAll:
                this.selectMode(All_Items_ItemId);
                All_Items_ItemId.setFocus();
                break;
        }
    }

]]></Source>
			</Method>
		</Methods>
		<DataSources xmlns="">
			<DataSource>
				<Name>TmpPIPApprovedItems</Name>
				<Methods>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            SalesTable              salesTable;
            SalesAgreementHeader    salesAgreementHeader;
            SalesLine               salesLine;
            AgreementLine           agreementLine;
            SalesQuotationLine      salesQuotationLine;
            CustAccount             custAccount;
        
            super();
        
            switch (element.args().dataset())
            {
                case (tableNum(SalesLine)):
                    salesLine   = element.args().record();
                    salesTable  = salesLine.salesTable();
                    custAccount = salesLine.CustAccount;
                    fromDate    = salesLine.pipACLDate();
                    break;
        
                case (tableNum(SalesQuotationLine)):
                    salesQuotationLine  = element.args().record();
                    custAccount         = salesQuotationLine.CustAccount;
                    fromDate            = salesQuotationLine.pipACLDate();
                    break;
        
                case (tableNum(AgreementLine)):
                case (tableNum(AgreementLineQuantityCommitment)):  
                case (tableNum(AgreementLineVolumeCommitment)):
                    agreementLine        = element.args().record();
                    salesAgreementHeader = SalesAgreementHeader::find(agreementLine.agreementHeader().RecId);
                    custAccount          = salesAgreementHeader.CustAccount;
                    fromDate             = agreementLine.pipACLFromDate(salesAgreementHeader);
                    toDate               = agreementLine.pipACLToDate(salesAgreementHeader);
                    break;
            }
        
            custTable = CustTable::find(custAccount);
        }

]]></Source>
					</Method>
				</Methods>
