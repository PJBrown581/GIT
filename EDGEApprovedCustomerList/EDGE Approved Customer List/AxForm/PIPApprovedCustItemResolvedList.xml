			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        SalesTable  salesTable;
    
        Args args = element.args();

        super();
    
        switch (args.dataset())
        {
            case tableNum(InventTable) :
                inventTable = args.record() as InventTable;
                inventTable = InventTable::find(inventTable.ItemId);
                tmpPIPApprovedCustItemList.linkPhysicalTableInstance(PIPApprovedCustomerList::buildResolvedCustomers(inventTable));
                isItemMode = true;
                break;
    
            case tableNum(CustTable) :
                custTable = args.record() as CustTable;
                custTable = CustTable::find(custTable.AccountNum);
                tmpPIPApprovedCustItemList.linkPhysicalTableInstance(PIPApprovedCustomerList::buildResolvedItems(custTable));
                isCustomerMode = true;
                break;
    
            case tableNum(SalesTable) :
                salesTable = args.record() as SalesTable;
                custTable = salesTable.custTable_CustAccount();
                tmpPIPApprovedCustItemList.linkPhysicalTableInstance(PIPApprovedCustomerList::buildResolvedItems(custTable));
                isCustomerMode = true;
                break;

            case tableNum(InventDimCombination):
                InventDimCombination dimCombination = args.record() as InventDimCombination;
                inventTable = dimCombination.inventTable();
                inventDimFilter = dimCombination.inventDim();
                element.showInventDimSelection();
                TmpPIPApprovedCustItemList.linkPhysicalTableInstance(PIPApprovedCustomerList::buildResolvedCustomers(inventTable, dateNull(), dateMax(), inventDimFilter));
                isItemMode = true;
                break;
    
            default :
                throw error(error::missingFormActiveBuffer(this.name(), strFmt('%1, %2, %3',
                    tableStr(InventTable), tableStr(CustTable), tableStr(SalesTable))));
        }
    
        this.initFormControls(args.menuItemName());
        this.updateDesign(InventDimFormDesignUpdate::Init);
    
        fromDate = DateTimeUtil::getSystemDate(DateTimeUtil::getUserPreferredTimeZone());
        if (isfilterAsOf)
        {
            toDate = DateTimeUtil::getSystemDate(DateTimeUtil::getUserPreferredTimeZone());
        }
        else
        {
            toDate = dateMax();
        }
    
        buttonViewAll.setFocus();
        viewState = DateEffectivenessViewState::All;
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    public void updateDesign(InventDimFormDesignUpdate mode)
    {
        switch (mode)
        {
            case InventDimFormDesignUpdate::Init:
                if (!inventDimFormSetup)
                {
                    inventDimFormSetup  = InventDimCtrl_Frm_ItemBarcode::newFromForm(element);
                }
                inventDimFormSetup.parmSkipOnHandLookUp(true);
            // do continue
            case InventDimFormDesignUpdate::Active:
                inventDimFormSetup.formActiveSetup(InventDimGroupSetup::newItemIdProductDimensionsOnly(TmpPIPApprovedCustItemList.ItemRelation));
                inventDimFormSetup.formSetControls(true);
                break;

            default : throw error(strFmt("@SYS54195",funcName()));
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>initFormControls</Name>
				<Source><![CDATA[
    private void initFormControls(MenuItemName menuItem)
    {
        if (isItemMode)
        {
            itemId.text(inventTable.ItemId);
            productName.text(inventTable.defaultProductName());
    
            this.design().caption("@PIP222");
            DetailsHeaderItem.visible(true);
            TmpPIPApprovedCustItemList_CustRelation.visible(true);
            TmpPIPApprovedCustItemList_CustName.visible(true);
        }
    
        if (isCustomerMode)
        {
            customerId.text(custTable.AccountNum);
            customerName.text(custTable.name());
    
            this.design().caption("@PIP220");
            DetailsHeaderCustomer.visible(true);
            TmpPIPApprovedCustItemList_ItemRelation.visible(true);
            TmpPIPApprovedCustItemList_ItemName.visible(true);
        }
    
        if (menuItem == menuitemDisplayStr(PIPResolvedItemsExpiration)
            || menuItem == menuitemDisplayStr(PIPResolvedCustomersExpiration))
        {
            isfilterAsOf = true;
    
            this.design().caption("@PIP218");
            ParmAsOfDate.visible(true);
            ParmIncludeNoExpiration.visible(true);
        }
        else
        {
            ParmFromDate.visible(true);
            ParmToDate.visible(true);
        }
    }

