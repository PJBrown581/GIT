						<Source><![CDATA[
        public void executeQuery()
        {
            fromDate = fromDate == dateNull() ? DateTimeUtil::getSystemDate(DateTimeUtil::getUserPreferredTimeZone()) : fromDate;
            toDate   = toDate   == dateNull() ? maxDate() : toDate;
        
            datesRangeValue = strFmt(element.datesQueryRangeFormat(),
                                date2StrXpp(fromDate),
                                date2StrXpp(toDate),
                                enum2int(includeNoExpiration));
        
            qbdrValidity.value(datesRangeValue);
        
            super();
        }

