			<Method>
				<Name>showInventDimSelection</Name>
				<Source><![CDATA[
    private void showInventDimSelection()
    {
        if (!inventDimFilter.anyProductDimensionSpecified())
        {
            ProductDimensionsFilter.visible(false);
            return;
        }

        ProductDimensionsFilter.visible(true);

        // Fixed filter dimensions; only show the specific dimension fields that are non-blank
        ListEnumerator productDimFieldIdEnumerator = InventDim::dimProductDimFieldList().getEnumerator();
        while (productDimFieldIdEnumerator.moveNext())
        {
            FieldId dimFieldId = productDimFieldIdEnumerator.current();
            filterInventDim_ds.object(dimFieldId).visible(inventDimFilter.(dimFieldId) != '');
        }

        filterInventDim_ds.executeQuery();
    }

]]></Source>
