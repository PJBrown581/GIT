			<Control>
				<Name>btnMatch</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            SKS_BR_MatchingDuplicateJournalDet localSKS_BR_MatchingDuplicateJournalDet;
            guid currentGuid = newGuid();
            container containerBankAccountTransRecIds;
            int counter;
            super();
        
            while select BankAccountTransRecId from localSKS_BR_MatchingDuplicateJournalDet where localSKS_BR_MatchingDuplicateJournalDet.SKS_BR_MatchingDuplicateJournalRECID == SKS_BR_MatchingDuplicateJournal.RecId
            {
                //A container is required, because, as soon as you stamp the first match, the rest will be deleted by the duplicateDelete in the matchManualMatch method.
                containerBankAccountTransRecIds += [localSKS_BR_MatchingDuplicateJournalDet.BankAccountTransRecId];
        
            }
            for(counter = 1; counter <= conLen(containerBankAccountTransRecIds); counter++)
            {
                if(counter < conLen(containerBankAccountTransRecIds))
                    element.args().caller().allowSKS_BR_BankTranStamp(false);
                else
                    element.args().caller().allowSKS_BR_BankTranStamp(true);
                element.args().caller().stampMatch(conPeek(containerBankAccountTransRecIds, counter), currentGuid);
            }
            element.close();
        }

]]></Source>
					</Method>
