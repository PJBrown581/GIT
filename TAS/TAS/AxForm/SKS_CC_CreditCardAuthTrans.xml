					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        /// <summary>
        ///
        /// </summary>
        public void init()
        {
            super();

            salesIdRange = this.query().dataSourceName('CreditCardAuthTrans').addRange(fieldnum(CreditCardAuthTrans, SalesId));

            QueryBuildDataSource    qbds;
            QueryBuildRange         qbrApprovalType, qbrConfirmDocNum;
            CustConfirmJour         custConfirmJourCaller;
            const str constEmpty = 'empty';            

            if (element.args() && element.args().parmEnumType())
            {
                transTypeCaller = element.args().parmEnum();
            }
            else
            {
                transTypeCaller = SKS_CC_TransTypeFilter::All;
            }

            qbds = this.query().dataSourceTable(tableNum(CreditCardAuthTrans));
            qbds.addSortField(fieldNum(CreditCardAuthTrans, RecId), SortOrder::Descending);

            if (transTypeCaller == SKS_CC_TransTypeFilter::SalesOrderConfirmation)
            {
                CreditCardAuthTrans_SKS_CC_JournalNum.visible(false);
                qbrConfirmDocNum = qbds.addRange(fieldNum(CreditCardAuthTrans, SKS_CC_CustConfirmJourDocNum));

                if (element.args() && element.args().dataset() == tableNum(CustConfirmJour))
                {
                    custConfirmJourCaller = element.args().record();

                    if (custConfirmJourCaller.ConfirmDocNum)
                    {
                        StatusFilterCtrl.selection(SKS_CC_TransStatusFilter::All);
                    }
                    else
                    {
                        // Force a non-existent value so no records are displayed
                        qbrConfirmDocNum.value(constEmpty);
                    }
                }
                else
                {
                    qbrConfirmDocNum.value(SysQuery::valueNotEmptyString());
                }

                qbrApprovalType = qbds.addRange(fieldNum(CreditCardAuthTrans, ApprovalType));
                qbrApprovalType.value(SysQuery::value(CreditCardOperation::Authorize));
            }

            qbrStatus = qbds.addRange(fieldNum(CreditCardAuthTrans, ProcessorStatus));
        }

]]></Source>
					</Method>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        /// <summary>
        ///
        /// </summary>
        public void executeQuery()
        {            
            if (this.query().dataSourceName('CreditCardAuthTrans').dynalinkCount() == 0)
            {
                salesIdRange.value(strFmt('(%1.%2 == %1.%3)',
                    salesIdRange.dataSource().name(),
                    fieldStr(CreditCardAuthTrans, SalesId),
                    fieldStr(CreditCardAuthTrans, OrigSalesId)));
            }            

            switch (StatusFilterCtrl.selection())
            {
                case SKS_CC_TransStatusFilter::Declined:
                    qbrStatus.value(SysQuery::value(CreditCardProcessorStatus::Declined));
                    break;
                case SKS_CC_TransStatusFilter::Approved:
                    qbrStatus.value(strFmt('%1, %2', enum2str(CreditCardProcessorStatus::Approved), enum2str(CreditCardProcessorStatus::Settled)));
                    break;
                default:
                    qbrStatus.value(SysQuery::valueUnlimited());
                    break;
            }

            super();
        }

]]></Source>
