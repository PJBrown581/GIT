                select firstOnly local_paymspecfields where local_paymspecfields.DefaultCodeLookupValuesRecId == sks_eb_codelookupvalues.recid;
        
                if(local_paymspecfields)
                {
                    error("@SKS2678");
                    ret = false;
                }
        
                if(ret)
                {
                    select firstOnly local_vendorpaymlookup where local_vendorpaymlookup.CodeLookupValuesRecId == SKS_EB_CodeLookupValues.RecId;
        
                    if(local_vendorpaymlookup)
                    {
                        error("@SKS2678");
                        ret = false;
                    }
                }
        
            }
        
            return ret;
        }

