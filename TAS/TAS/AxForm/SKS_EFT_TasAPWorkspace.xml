			<Method>
				<Name>visibleVendBankApproval</Name>
				<Source><![CDATA[
    /// <summary>
    /// This method will make the Vendor Bank Aprroval buttons visible or invisible
    /// </summary>
    /// <Issue20159: Vendor Bank Aprroval buttons visible or invisible>
    private void visibleVendBankApproval()
    {
        SKS_EFT_Setup sKS_EFT_Setup = SKS_EFT_Setup::find();

        APEFTVendBankAcctPending.visible(sKS_EFT_Setup.APAcctApproval);
        APEFTVendBankAcctPendingAssignedMe.visible(sKS_EFT_Setup.APAcctApproval);
        SKS_EFTAP_VendBankAccntPendingApprovalTileButton.visible(sKS_EFT_Setup.APAcctApproval);
        SKS_EFTAP_VendBankAccntPendingApprovalAssnMeTileButton.visible(sKS_EFT_Setup.APAcctApproval);
    }

]]></Source>
