						<Source><![CDATA[
        public int active()
        {
            int ret;
        
            ret = super();
        
            if(SKS_LB_FileHistory.Action == SKS_LB_Action::PmtJrnlCreated
                || SKS_LB_FileHistory.Action == SKS_LB_Action::PmtJrnlPost
                || SKS_LB_FileHistory.Action == SKS_LB_Action::PmtJrnlPostFail)
            {
                LedgerJournalTable.enabled(true);
            }
            else
            {
                LedgerJournalTable.enabled(false);
            }
        
            if(SKS_LB_FileHistory.Action == SKS_LB_Action::RevJrnlCreated)
            {
                LedgerJournalTable3.enabled(true);
            }
            else
            {
                ledgerJournalTable3.enabled(false);
            }
        
            return ret;
        }

]]></Source>
