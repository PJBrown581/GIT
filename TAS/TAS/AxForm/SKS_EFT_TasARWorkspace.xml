						<Source><![CDATA[
        /// <summary>
        /// Count of Workbench reviews assigned to me tile
        /// </summary>
        /// <returns></returns>
        public TileButtonControlData getData()
        {
            TileButtonControlData ret;
        
            ret = super();

            SKS_LB_Journal          journal;
            SKS_LB_JournalAR        journalAR;
            SKS_LB_JournalARErrors  journalARErrors;
            container con;

            int recCount = 0;

            while select journal
                join journalAR
                where journalAR.JournalId == journal.JournalID
                && journalAR.BatchNum == journal.BatchNum
                && journalAR.ReviewStatus == SKS_LB_ReviewStatus::NeedsReview
                && journalAR.AssignedTo == curUserId()
            {
                con += journal.RecId;
            }
          
            while select journal               
                join journalARErrors
                where journalARErrors.JournalId == journal.JournalID              
                && journalARErrors.BatchNum == journal.BatchNum
                && journalARErrors.ReviewStatus == SKS_LB_ReviewStatus::NeedsReview
                && journalARErrors.AssignedTo == curUserId()
            {
                if(conFind(con, journal.RecId) == 0)
                {
                    con += journal.RecId;
                }
            }
            recCount = conLen(con);              
        
            ret.strTileData(strFmt('%1', recCount));
        
            return ret;
        }

]]></Source>
