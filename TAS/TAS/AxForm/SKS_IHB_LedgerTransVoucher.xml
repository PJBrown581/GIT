					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        /// <summary>
        /// View the all IHB posted transactions.
        /// </summary>
        // SKS_FDD_9536
        public void executeQuery()
        {
            GeneralJournalEntry     inputGeneralJournalEntry;
            QueryBuildDataSource    dataSourceGeneralJournalEntry;

            dataSourceGeneralJournalEntry = this.query().dataSourceTable(tableNum(GeneralJournalEntry));
            inputGeneralJournalEntry = element.args().record() as GeneralJournalEntry;

            if (inputGeneralJournalEntry.SKS_EFTAP_OrigPaymVouch != '')
            {
                dataSourceGeneralJournalEntry.addRange(fieldNum(GeneralJournalEntry, SKS_EFTAP_OrigPaymVouch)).value(inputGeneralJournalEntry.SKS_EFTAP_OrigPaymVouch);
            }
            else if (inputGeneralJournalEntry.SKS_EFTAR_OrigPaymVouch != '')
            {
                dataSourceGeneralJournalEntry.addRange(fieldNum(GeneralJournalEntry, SKS_EFTAR_OrigPaymVouch)).value(inputGeneralJournalEntry.SKS_EFTAR_OrigPaymVouch);
            }

            super();
        }

]]></Source>
