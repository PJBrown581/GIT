            public void jumpRef()
            {
                Args args = new Args();
                args.record(LedgerJournalTable::find(SKS_SCF_JournalHistory.JournalId));

                if(SKS_SCF_JournalHistory.Action == SKS_SCF_JournalAction::PayJourCreated ||
                    SKS_SCF_JournalHistory.Action == SKS_SCF_JournalAction::PayJourCreateFailed ||
                    SKS_SCF_JournalHistory.Action == SKS_SCF_JournalAction::PayJourPostFailed ||
                    SKS_SCF_JournalHistory.Action == SKS_SCF_JournalAction::PayJourPosted)
                {
                    new MenuFunction(MenuItemDisplayStr(LedgerJournalTable5),MenuItemType::Display).run(Args);
                }
                else
                {
                    new MenuFunction(MenuItemDisplayStr(LedgerJournalTable9),MenuItemType::Display).run(Args);
                }

                super();
            }

