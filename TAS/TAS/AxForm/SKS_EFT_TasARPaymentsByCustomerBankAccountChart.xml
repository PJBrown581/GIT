			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        
        super();
        
        if (element.args().caller().name() == formStr(SKS_EFT_TasARWorkspace))
        {
            //Setting the caller form to retrieve the currency type selected; either accounting or reporting from workspace form.
            element.parmCallerForm(element.args().caller());

            if (element.parmCallerForm())
            {
                element.parmCompany(element.parmCallerForm().parmCompany());
                element.parmdateCode(element.parmCallerForm().parmdateCode());
            }
            

            SKS_EFT_TasARPaymentsByCustomerTmp.linkPhysicalTableInstance(clCustTmp);
            element.buildTempTable();
            element.updateTitle();
            element.localRefresh();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateTitle</Name>
				<Source><![CDATA[
    public void updateTitle()
    {
        LedgerPeriodCode dateCodeLoc;
        select firstonly crosscompany dateCodeLoc
            where dateCodeLoc.Code == element.parmdateCode() &&
                  dateCodeLoc.DataAreaId == element.parmCompany();

        SysChartTitle title = PaymentByBankAccount.Title();
        changecompany (this.parmCompany())
        {
            select count(RecId) from clCustTmp;
        }
        title.parmText(strFmt('%1 - %2%3', "@SKS:SKGPaymentsByBankAccount", dateCodeLoc.Code, clCustTmp.RecId >= 10?strFmt(' (%1 %2)', "@SYS26115", int642Str(clCustTmp.RecId)):strMin()));

        PaymentByBankAccount.Title(title);
    }

]]></Source>
			</Method>
			<Method>
				<Name>buildTempTable</Name>
				<Source><![CDATA[
    public void buildTempTable(boolean _update = false)
    {
        CustTrans   locCustTrans;
        LedgerPeriodCode ledgerPeriodCodelc;
        LedgerJournalTrans locLedgerJournalTrans;
        DimensionAttributeValueCombination locDimensionAttributeValueCombination;
        
        delete_from clCustTmp;
        this.updateTitle();
        if(this.parmCompany())
        {
            changecompany (this.parmCompany())
            {   
                select firstonly ledgerPeriodCodelc
                    where ledgerPeriodCodelc.Code == this.parmdateCode();
                
                insert_recordset clCustTmp(AmountBalanceMst, DefaultDateInterval, OffsetLedgerDimension, AccountID)
                    select firstonly10 sum(AmountMST), dateCodeSelected  from locCustTrans
                        order by AmountMST desc
                        where locCustTrans.TransDate >= ledgerPeriodCodelc.fromDate()
                            && locCustTrans.TransDate <= ledgerPeriodCodelc.toDate()
                            && locCustTrans.TransType == LedgerTransType::Payment
                            join OffsetLedgerDimension from locLedgerJournalTrans
                                group by OffsetLedgerDimension
                                    where locLedgerJournalTrans.CustTransId == locCustTrans.RecId
                                        && locLedgerJournalTrans.OffsetAccountType == LedgerJournalACType::Bank
                            join DisplayValue from locDimensionAttributeValueCombination
                                group by DisplayValue
                                where locDimensionAttributeValueCombination.RecId == locLedgerJournalTrans.OffsetLedgerDimension;

                update_recordset clCustTmp
                    setting AmountBalanceMst = clCustTmp.AmountBalanceMst * -1;
             

            }
        }
    }

]]></Source>
