						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            Query                   query = new Query();
            QueryBuildDataSource    queryBuildDataSource, qbds2;
            ;
            query.allowCrossCompany(true);
            this.query().dataSourceTable(tablenum(CustTable)).clearRanges();
            // Limit and arrange the data selection
            queryBuildDataSource = query.addDataSource(tableNum(CustTable));

            qbds2 = queryBuildDataSource.addDataSource(tableNum(CustBankAccount));
            qbds2.addLink(fieldNum(CustTable, AccountNum), fieldNum(CustBankAccount, CustAccount));
            qbds2.joinMode(JoinMode::ExistsJoin);
            qbds2.addRange(fieldNum(CustBankAccount, RegistrationNum)).value(queryvalue(SKS_LB_JournalARErrors.CustBankRegNum));
            qbds2.addRange(fieldNum(CustBankAccount, AccountNum)).value(queryvalue(SKS_LB_JournalARErrors.CustBankAccountNum));

            this.query(query);

            super();
        }

]]></Source>
					</Method>
					<Method>
						<Name>CustomerName</Name>
						<Source><![CDATA[
        public display DirPartyName CustomerName(CustTable  _CustTable)
        {
            DirPartyTable   DirPartyTable;
            select firstonly name from DirPartyTable where dirPartyTable.recid == _CustTable.party;
            return DirPartyTable.name;
        }

]]></Source>
