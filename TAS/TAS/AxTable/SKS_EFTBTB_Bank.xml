			<Method>
				<Name>UpdateFromSetup</Name>
				<Source><![CDATA[
    static server void UpdateFromSetup(SKS_EFT_Setup _sks_eft_setup)
    {
        SKS_EFTBTB_Bank sks_eft_bankUpdate;
        SKS_EFT_FileFormat sks_eft_FileFormat;
        SKS_BTB_PaymModeTable locSKS_BTB_PaymModeTable;
        PaymMode lastpaymMode;
        ;
        while select forupdate sks_eft_bankUpdate order by PaymMode
        {
            if(sks_eft_bankUpdate.PaymMode != lastpaymMode)
            {
                select FormatId from locSKS_BTB_PaymModeTable
                    where locSKS_BTB_PaymModeTable.PaymMode == sks_eft_bankUpdate.PaymMode
                join CrLf, IncludeCpnyDebit from sks_eft_FileFormat
                    where sks_eft_FileFormat.FormatId == locSKS_BTB_PaymModeTable.FormatId;
            }

            if(sks_eft_bankUpdate.BBFileFromSetup)
            {
                sks_eft_bankUpdate.BBFileName = _sks_eft_setup.BTBFileName;
                sks_eft_bankUpdate.BBFilePath = _sks_eft_setup.BTBFilePath;
                sks_eft_bankUpdate.BTBBankFabricFileLocation = _sks_eft_setup.BTBBankFabricFileLocation;
                sks_eft_bankUpdate.BBArchive = _sks_eft_setup.BTBArchive;
                sks_eft_bankUpdate.BBPathArchive = _sks_eft_setup.BTBPathArchive;
                //sks_eft_bankUpdate.BBHeadTrailID = _sks_eft_setup.bb;
                sks_eft_bankUpdate.BBACHInclDR = _sks_eft_setup.BTBInclDRC;
                sks_eft_bankUpdate.BBDelete = _sks_eft_setup.BTBDelete;
                //sks_eft_bankUpdate.BBCommType = _sks_eft_setup.btbcomm;
                sks_eft_bankUpdate.BTBCommID = _sks_eft_setup.BTBCommID;
                //sks_eft_bankUpdate.BBCommUserProg = _sks_eft_setup.APCommUserProg;
                sks_eft_bankUpdate.BankHolidayGroupId = _sks_eft_setup.BTBBankHolidayGroupID;
                sks_eft_bankUpdate.CharacterReplacementGroupId = _sks_eft_setup.BTBCRGroupId;

            }

            if(sks_eft_bankUpdate.CompanyFromSetup)
            {
                sks_eft_bankUpdate.ACHCompanyID = _sks_eft_setup.ACHCompanyID;
                sks_eft_bankUpdate.ACHCompanyName = _sks_eft_setup.ACHCompanyName;
                sks_eft_bankUpdate.ACHOrigDFI = _sks_eft_setup.ACHOrigDFI;
                sks_eft_bankUpdate.ACHOrigDFIName = _sks_eft_setup.ACHOrigDFIName;
                sks_eft_bankUpdate.ACHFileIDModifier = _sks_eft_setup.ACHFileIDModifier;
            }

            sks_eft_bankUpdate.update();

            lastpaymMode = sks_eft_bankUpdate.PaymMode;
        }
    }

]]></Source>
