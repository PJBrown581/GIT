			<Method>
				<Name>insert</Name>
				<Source><![CDATA[
    /// <summary>
    /// Inserting the bank code lookups
    /// </summary>
    public void insert()
    {
        SKS_BTB_PaymModeTable       lSKS_BTB_PaymModeTable;
        SKS_EFT_FileFormat      lSKS_EFT_FileFormat;
        ;

        ttsbegin;
        if(this.PaymMode != '')
        {
            lSKS_BTB_PaymModeTable = SKS_BTB_PaymModeTable::find(this.PaymMode);
        }

        super();

        select firstOnly RecId from lSKS_EFT_FileFormat
            where lSKS_EFT_FileFormat.FormatId == lSKS_BTB_PaymModeTable.FormatId;

        this.BankAccountLookupAddition(lSKS_EFT_FileFormat);

        ttscommit;
    }

]]></Source>
