			<Method>
				<Name>findByCompany</Name>
				<Source><![CDATA[
    // SKS_FDD_10590 -->
    public static SKS_EFTBTB_Bank findByCompany(CompanyId _company, BankAccountId _bankAccountId, PaymMode _paymMode, boolean _forUpdate = false)
    {
        SKS_EFTBTB_Bank sks_eft_bank;

        if (_company)
        {
            changecompany(_company)
            {
                sks_eft_bank = SKS_EFTBTB_Bank::find(_bankAccountId, _paymMode, _forUpdate);
            }
        }

        return sks_eft_bank;
    }

]]></Source>
