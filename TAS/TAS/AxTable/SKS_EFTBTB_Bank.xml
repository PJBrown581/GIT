				<Name>BankAccountLookupAddition</Name>
				<Source><![CDATA[
    // SKS_FDD_10590 <--

    /// <summary>
    /// Inserting the bank code lookups
    /// </summary>
    private void BankAccountLookupAddition(SKS_EFT_FileFormat SKS_EFT_FileFormat)
    {
        SKS_EFT_BTBBankAccountPaymLookup       lSKS_EFT_BTBBankAccountPaymLookup;
        SKS_EFT_FileFormatFields            SKS_EFT_FileFormatFields;
        RecordInsertList                    ril;

        ril = new RecordInsertList(lSKS_EFT_BTBBankAccountPaymLookup.TableId);

        while select RecId, fixedvalue, defaultcodelookupvaluesrecid from SKS_EFT_FileFormatFields
            where SKS_EFT_FileFormatFields.fileformatrecid == SKS_EFT_FileFormat.recid
        {
            lSKS_EFT_BTBBankAccountPaymLookup.CodeLookupValuesRecId = SKS_EFT_FileFormatFields.DefaultCodeLookupValuesRecId;
            lSKS_EFT_BTBBankAccountPaymLookup.FileFormatFieldsRecId = SKS_EFT_FileFormatFields.RecId;
            lSKS_EFT_BTBBankAccountPaymLookup.BankPaymRecId = this.RecId;
            lSKS_EFT_BTBBankAccountPaymLookup.FixedValue = SKS_EFT_FileFormatFields.FixedValue;
            ril.add(lSKS_EFT_BTBBankAccountPaymLookup);
        }
        ril.insertDatabase();
    }

]]></Source>
