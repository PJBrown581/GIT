				<Name>find</Name>
				<Source><![CDATA[
    public static SKS_EFTBTB_Bank find(BankAccountId _bankAccountId, PaymMode _paymMode,
                                     boolean            _forUpdate = false)
    {
        SKS_EFTBTB_Bank sks_eft_bank;

        if (_bankAccountId)
        {
            if (_forUpdate)
            {
                sks_eft_bank.selectForUpdate(_forUpdate);
            }

            select sks_eft_bank
                where sks_eft_bank.BankAccountId == _bankAccountId &&
                      sks_eft_bank.PaymMode == _paymMode;
        }

        return sks_eft_bank;
    }

]]></Source>
