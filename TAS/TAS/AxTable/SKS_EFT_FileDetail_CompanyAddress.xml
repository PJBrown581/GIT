				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    ///  Returns a record in <c>SKS_EFT_FileDetail_CompanyAddress</c>
    ///  based on the _recid parameter
    /// </summary>
    /// <param name = "_recid">
    /// The file detail recid to find
    /// </param>
    /// <param name = "_forupdate">
    ///  True if the record should be selected for update
    /// </param>
    /// <returns>
    ///  The <c>file detail recid</c> record
    /// </returns>
    public static SKS_EFT_FileDetail_CompanyAddress find(
        RefRecId             _recid,
        boolean             _forupdate        = false)
    {
        SKS_EFT_FileDetail_CompanyAddress  companyAddress;
        if(_recid)
        {
            if(_forupdate)
            {
                companyAddress.selectForUpdate(_forupdate);
            }

            select companyAddress
            where companyAddress.RefRecId == _recid;
        }
        return companyAddress;
    }

]]></Source>
