    public static void insertEvent(SKS_CCR_Action _SKS_CCR_Action,
                                    SKS_CCR_BankHeaderRecId _SKS_CCR_BankHeaderRecId,
                                    CompanyBankAccountId _CompanyBankAccountId = '',
                                    TransDate _AccountStatementDate = dateNull(),
                                    BankAccountStatementNum _AccountStatementNum = '',
                                    LedgerJournalId _LedgerJournalId  = '',
                                    SKS_EB_AdditionalData _AdditionalData = ''
                                    )
    {
        SKS_CCR_HistoryTable locSKS_CCR_HistoryTable;
    
        locSKS_CCR_HistoryTable.Action = _SKS_CCR_Action;
        locSKS_CCR_HistoryTable.BankHeaderRecId = _SKS_CCR_BankHeaderRecId;
        if(_CompanyBankAccountId != '')
        {
            locSKS_CCR_HistoryTable.AccountId = _CompanyBankAccountId;
        }
        if(_AccountStatementDate != dateNull())
        {
            locSKS_CCR_HistoryTable.AccountStatementDate = _AccountStatementDate;
        }
        if(_AccountStatementNum != '')
        {
            locSKS_CCR_HistoryTable.AccountStatementNum = _AccountStatementNum;
        }
        if(_LedgerJournalId != '')
        {
            locSKS_CCR_HistoryTable.JournalNum = _LedgerJournalId;
        }
        if(_AdditionalData != '')
        {
            locSKS_CCR_HistoryTable.AdditionalData = _AdditionalData;
        }
        ttsBegin;
        locSKS_CCR_HistoryTable.insert();
        ttsCommit;
    
    
    }

]]></Source>
